#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules/jest-cli/bin/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules/jest-cli/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules/jest-cli/bin/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules/jest-cli/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/jest-cli@30.2.0_@types+node@24.6.2/node_modules:/Users/ryanmay/Documents/GitHub/Heidi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
