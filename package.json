{
  "name": "@heidi/core",
  "version": "0.0.1",
  "description": "An extension of middy that adds tools to configure AWS endpoint handlers with ease. (core package)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "middy": "^0.36.0",
    "typia": "^9.7.2",
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
